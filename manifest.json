{
  "manifest_version": 2,
  "name": "Orbit",
  "description": "A chrome extension to make it easy to generate orbs transactions",
  "version": "0.0.4",
  "browser_action": {
    "default_title": "Orbit",
    "default_popup": "src/popup/popup.html",
    "default_icon": "icons/icon16.png"
  },
  "background": {
    "scripts": ["dist/js/background.js"],
    "persistent": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": []
}
